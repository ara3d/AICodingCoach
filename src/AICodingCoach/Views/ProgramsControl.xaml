<UserControl x:Class="AICodingCoach.Views.ProgramsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:views="clr-namespace:AICodingCoach.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<TabControl TabStripPlacement="Left" Margin="0, 0, 0, 10">
		<TabControl.ContentTemplate>
			<DataTemplate>
				<views:MainContentControl></views:MainContentControl>
			</DataTemplate>
		</TabControl.ContentTemplate>
		<TabItem Header="Hello!" />
		<TabItem Header="Star" />
		<TabItem Header="Line" />
		<TabItem Header="Circle" />
		<TabItem Header="Square" />
		<TabItem Header="Rings" />
		<TabItem Header="Button" />
		<TabItem Header="Mouse" />
		<TabItem Header="Colors"></TabItem>
		<TabItem Header="Animation"></TabItem>
		<TabItem Header="Time"></TabItem>
		<TabItem Header="Internet"></TabItem>
		<TabItem Header="Files"></TabItem>
		<TabItem Header="Pixels"></TabItem>
		<TabItem Header="Graph"></TabItem>
		<TabItem Header="Grid"></TabItem>
		<TabItem Header="Math"></TabItem>
		<TabItem Header="Text"></TabItem>
		<TabItem Header="Particles"></TabItem>
		<TabItem Header="Sound"></TabItem>
		<TabItem Header="AI"></TabItem>
	</TabControl>
</UserControl>
